db._query(`FOR laptop in cloudseer_laptops UPDATE laptop WITH { vpn_event_attributes : (FOR people in cloudseer_peoples FOR vpnd IN people.activities.vpn FILTER laptop.mac == vpnd.mac RETURN { session: people.activities.vpn[0].session, user: people.displayName, email: people.emailAddress, logon : people.logon, since: people.since, csId: people._id })} INTO cloudseer_laptops OPTIONS { mergeObjects: true }`).getExtra()
