db._query(`FOR person IN cloudseer_peoples UPDATE person WITH { user_badgecount_by_hour: (FOR n in 0..23 FOR q in cloudseer_time_buckets FILTER n == q.hour AND TO_NUMBER(q.event.BadgeId) == person.badgeId COLLECT bucket_hour = n WITH count INTO length RETURN {bucket_hour:bucket_hour, count:length})} IN cloudseer_peoples  OPTIONS { mergeObjects: true }`).getExtra()
