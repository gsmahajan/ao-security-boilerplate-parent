db._query(`FOR person IN cloudseer_peoples UPDATE person WITH { user_badgecount_by_weekday: (FOR q in cloudseer_time_buckets FOR n in 1..7 FILTER n == q.week AND TO_NUMBER(q.event.BadgeId) == person.badgeId COLLECT bucket_week = q.weekday WITH count INTO length RETURN {bucket_week:bucket_week, count:length})} IN cloudseer_peoples  OPTIONS { mergeObjects: true }`).getExtra()
